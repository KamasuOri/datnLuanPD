		$(function() {		
			$('#dg-container').gallery();			
			var dt = $(".btmarea .price-zone dt span").outerWidth();
			var btn = $(".btmarea .price-zone .button").outerWidth();
			if (dt > btn ) {
				$(".btmarea .price-zone").css('width', dt + 20);
			}else{
				$(".btmarea .price-zone").css('width', btn + 20);
			}	
			$('#showlist').text(update_open_list);				
		});
		function swapStatus(obj) {
			var target = obj.parents(".notice").next(".change-details");
			if (target.hasClass("closed")) {
				target.removeClass("closed").addClass("open");
				obj.text(update_close_list);
			}else if (target.hasClass("open")) {
				target.removeClass("open").addClass("closed");
				obj.text(update_open_list);
			}
		}	
	</script>
